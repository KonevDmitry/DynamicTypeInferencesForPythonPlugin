<idea-plugin>
    <id>com.github.konevdmitry.dynamictypereferencesforpythonplugin</id>
    <name>DynamicTypeReferences</name>

    <depends>com.intellij.modules.python</depends>
    <depends>com.intellij.modules.lang</depends>

    <vendor>FuckingDiploma</vendor>
    <version>0.0.1</version>
    <!-- Product and plugin compatibility requirements -->
    <!-- https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html -->


    <idea-version since-build="183.0" until-build="203.*"/>
    <extensions defaultExtensionNs="com.intellij">
        <lang.inspectionSuppressor language="Python" implementationClass="dynamic.type.references.FlakeInspectionSuppressor" />
    </extensions>

    <actions>
        <action class="dynamic.type.references.CodeGetter" id="CodeGetter"
                text="PSI Navigation Demo...">
            <add-to-group group-id="ToolsMenu" anchor="last"/>
        </action>
    </actions>


